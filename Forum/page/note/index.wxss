.best-btn,.righted{position:absolute;right:-24rpx;top:150rpx;width:128rpx;height:128rpx;background: url(data:image/png;base64,) no-repeat 50% 50%; -webkit-background-size: 128rpx 128rpx; background-size: 128rpx 128rpx;z-index: 6;}
.righted{background: url(data:image/png;base64,);-webkit-background-size: 128rpx 128rpx; background-size: 128rpx 128rpx;}


.tabBar{
    position: fixed;
    bottom: 0;
    left: 0;
    right: 0;
    height: 100rpx;
    border-top: 1px solid #DDD;
    background: #FFF;
    z-index: 999;
    display: flex;
}
.tabBar .item {
    text-align: center;
    display: block;
    color: #A2B4CC;
    font-size: 26rpx;
    flex: 1;
    -webkit-box-flex: 1;
}
.tabBar text{
    font-size: 26rpx;
    color: #A2B4CC;
    letter-spacing: 0;
}
.tabBar .icon{
    position: relative;
    font-family: 'forum';
    font-size: 28rpx;
    color: #A2B4CC;
    margin-top: 10rpx;
}
.tabBar .icon text{
    position: absolute;
    left: 57%;
    top: 0;
    font-size: 18rpx;
    background: #5F7FC1;
    border-radius: 100px;
    line-height: 18rpx;
    color: #fff;
    padding: 4rpx;
}
.tabBar .home:before{
    content: '\e642';
}
.tabBar .zan:before{
    content: '\e63b';
}
.tabBar .comment:before{
    content: '\e640';
}
.tabBar .forward:before{
    content: '\e648';
}
.blueAct text,.blueAct .icon{color: #3B5998;}
scroll-view{padding-bottom: 120rpx;box-sizing: border-box;}




.mi_box{
    padding: 0 15px 15px;
    word-break: break-all;
    word-wrap: break-word;
    position: relative;
    overflow: hidden;
    background: #fff;
    border-top: 2rpx solid #EEEEEE;
}
.mi_box_header{
    height: 126rpx;
    border-bottom: 2rpx solid #EEEEEE;
}
.mi_ico{
    width: 64rpx;
    height: 64rpx;
    float: left;
    overflow: hidden;
    margin: 32rpx 10rpx 0 0;
    border-radius: 50%;
}
.mi_info{
    float: left;
    margin-top: 32rpx;
}
.mi_info .name{margin-top: 8rpx;}

.mi_browse{
    float: right;
    line-height: 126rpx;
    font-size: 22rpx;
    color: #999;
}
.mi_browse:before{
    content: '\e600';
    font-family: 'forum';
    color: #ddd;
    float: left;
    line-height: 126rpx;
    font-size: 20rpx;
    margin: 0 10rpx 0 0;
}
.mi_title{
    font-size: 40rpx;
    color: #000000;
    line-height: 90rpx;
}
.mi_time{
    font-family: PingFangSC-Regular;
    font-size: 26rpx;
    color: #888888;
    letter-spacing: 0;
    margin-bottom: 24rpx;
}
.mi_tag{
    border: 2rpx solid #FF9300;
    border-radius: 2px;
    display: inline-block;
    font-size: 22rpx;
    color: #FF9300;
    letter-spacing: 0;
    padding: 2rpx 4rpx;
    line-height: 22rpx;
    margin-left: 20rpx;
    position: relative;
    top: -4rpx;
}
.mi_msg{
font-size: 32rpx;
color: #353535;
letter-spacing: 0;
line-height: 48rpx;
}

.mi_cont_box{
    margin-top: 15px;
background: #FAFCFD;
border: 2rpx solid #CFD8E6;
border-radius: 3px;
}
.mi_cot{
    background: #405C80;
    border-radius: 3px 3px 0 0;
    height: 88rpx;
    line-height: 88rpx;
    font-size: 32rpx;
    color: #FFFFFF;
    letter-spacing: 0;
    display: block;
    text-indent: 20rpx;
}
.mi_cot_box{
    padding:20rpx 20rpx 0;
}
.mi_cot_hed{
    height: 84rpx;
}
.mi_cot_time{line-height: 84rpx;
font-size: 22rpx;
color: #B2B2B2;
letter-spacing: 0;
float: right;
}
.mi_cot_msg{
font-size: 28rpx;
color: #353535;
letter-spacing: 0;
line-height: 36rpx;
padding-bottom: 20rpx;
border-bottom: 2rpx solid #CFD8E6;
}

.hot_msg_box,.all_msg_box{
    border-bottom: 2rpx solid #EEEEEE;
    height: 88rpx;
    line-height: 88rpx;
    text-indent: 30rpx;
    font-size: 32rpx;
    color: #FF9300;
    letter-spacing: 0;
    background: #fff;
    margin-top: 20rpx;
}
.hot_msg_box:after{
    content: '\e641';
    font-family: 'forum';
    margin-left: 8rpx;
}
.all_msg_box{
    color: #000000;
}
.all_msg_box:before{
    content: ' ';
    width: 4rpx;
    height: 36rpx;
    float: left;
    background: #375697;
    margin-top: 26rpx;
}
.mi_xl_btn{
    font-size: 22rpx;
    color: #A2B4CC;
    letter-spacing: 0;
    float: right;
    padding-right: 15px;
}
.mi_xl_btn:before{
    content: '\e647';
    font-family: 'forum';
    margin-right: 8rpx;
}
.mi_xl_dx:before{
    content: '\e63a';
}



.mt_box{
    margin-top: 0;
}
.mt_fb{
    float: right;
font-size: 22rpx;
color: #A2B4CC;
letter-spacing: 0;
line-height: 64rpx;
}
.mf_con_box{
    padding-left: 74rpx;
}
.mf_con_box:after{
