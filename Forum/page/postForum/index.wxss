.checkBoxFrom{
  margin: 20rpx 0 30rpx;
  white-space: nowrap;
  height: 72rpx;
}

.page__bd{
  display: block;
  width: 100%;
  overflow: hidden;
}




/*-------------------------------------------------*/
.post-title{
  display: block;
  font-family: PingFangSC-Regular;
  font-size: 28rpx;
  color: #215C9E;
  letter-spacing: 0;
  line-height: 28rpx;
  padding: 32rpx 674rpx 10rpx 20rpx;
}
.post-input{
  height: 88rpx;
  background: #fff;
  padding:0 30rpx;
}
.post-maintext{
  width: 100%;
  height: 320rpx;
  background: #fff;
}
.post-textarea{
  width: 690rpx;
  padding-top: 20rpx;
  padding-left: 30rpx;
}
.post-sync{
  padding: 22rpx 0 10rpx 20rpx;
  font-size: 28rpx;
  color: #888888;
  line-height: 22rpx;
}
.post-forum{
  display: inline-block;
  position: relative;
  width: 212rpx;
  height: 68rpx;
  line-height: 68rpx;
  text-align: center;
  word-spacing: normal;
  overflow: hidden;
  border: 2rpx solid #A2B4CC;
  border-radius: 8rpx;
  margin-left: 20rpx;
  font-size: 26rpx;
  color: #A2B4CC;

}
.post-click:after{
  position: absolute;
  content: '\e63d';
  font-family: 'forum';
  font-size: 10rpx;
  color: #375697;
  bottom: 6rpx;
  right: 6rpx;
  display: block;
  line-height: 30rpx;
  width: 30rpx;
  height: 30rpx;
  z-index: 33;
}
.post-click{
  border: 1rpx solid #375697;
  color: #375697;
}
.post-complete{
  width: 690rpx;
  height: 88rpx;
  background: #375697;
  border: 1rpx solid #2F4B87;
  border-radius: 5px;
  font-family: .PingFang-SC-Regular;
  font-size: 36rpx;
  color: #FFFFFF;
  margin-top: 100rpx;
}
.post-keyboard{
  position:fixed;
  left: 0;
  bottom: 0;
  width: 100%;
  background: #fff;
  z-index: 1100;
}
.post-select{
  height: 70rpx;
  border-bottom: 4rpx solid #eee;
}
.post-commen{
  display: inline-block;
  padding-left: 40rpx;
}
.post-feels{
  padding-left: 30rpx;
}
.post-feels:before{
  content: '\e638';
  font-family: 'forum';
  font-size: 38rpx;
  line-height: 70rpx;
  color: #B2B2B2;
}
.post-pic:before{
  content: '\e643';
  font-family: 'forum';
  font-size: 36rpx;
  line-height: 70rpx;
  color: #B2B2B2;
}
.post-photo:before{
  content: '\e645';
  font-family: 'forum';
  font-size: 36rpx;
  line-height: 70rpx;
  color: #B2B2B2;
}

.post-keyContent{
  height: 390rpx;
}
.images-list{
  display: flex;
  flex-wrap: wrap;
}
.post-selectPhoto-image{
  width: 120rpx;
  height: 120rpx;
  margin-top: 30rpx;
  margin-left: 30rpx;
  border: 1rpx solid #f5f5f5;
  position: relative;
  overflow: hidden;
}
.post_selectPhoto_image{
  width: 120rpx;
  height: 120rpx;
}
.poimg_box:after{
  content: '\e639';
  font-family: 'forum';
  display: block;
  width: 46rpx;
  height: 46rpx;
  position: absolute;
  font-size: 24rpx;
  border-radius: 100%;
  background: rgba(0,0,0,.6);
  color: #fff;
  top: -6rpx;
  right: -6rpx;
  text-align: center;
  line-height: 46rpx;
}
.post-selectPhoto{
  position: relative;
  border: 1rpx dashed #DDDDDD;
  border-radius: 4rpx;
}
.post-selectPhoto:before{
  display: block;
  content: '\e632';
  font-family: 'forum';
  font-size: 42rpx;
  color: #DDD;
  text-align: center;
  line-height: 120rpx;
}
.Pactive:before{color:#375697;}

textarea{opacity: .9;z-index: 2;}
/*表情*/
swiper{ height: 390rpx; background: #F6F6F7;}
.swiper-item{height: 370rpx;padding: 20rpx 0 0 14rpx;}

.icon-phiz{display:inline-block;width:24px;height:24px;background:url(http://static.360che.com/wap/images/bbs/icon-phiz.gif) no-repeat;vertical-align:top;float:left;margin:14rpx 16rpx;}
.emoji{position:relative;display:none;background-color:#f5f5f5;overflow:hidden;height:194px;width:100%;}
.emoji.visible {display: block;}
.emoji:after,.my-help-list li:after,.success-list header:after{bottom:-1px;top:initial;}
.emoji .container{position:absolute;width:1000%;}
.emoji .container li{float:left;width:10%;}
.emoji .container img{vertical-align:middle;max-width:100%}
.emoji .tabs{position:absolute;bottom:20px;left:0;width:100%;text-align:center;font-size:0}
.emoji .tabs > i{margin:0 2px;width:10px;height:10px;display:inline-block;vertical-align:top;-webkit-border-radius:50%;border-radius:50%;background:#ccc;}
.emoji .tabs .selected{background:#2f75bb;}
#increment{width:45px;height:80px;display:none;pointer-events:none;position:absolute;z-index:99;font:20px/2 Arial;color:#8faacc;right:50%;top:50%;}
#increment.visible{display:block;-webkit-animation:increment-fade-out .5s 1 linear;animation:increment-fade-out .5s 1 linear;-webkit-animation-fill-mode:forwards;animation-fill-mode:forwards;}
.help-form{background:#f5f5f5;height:100%}
.help-form-container{min-height:100%}
.help-form .weui_cell{overflow:hidden;background:#fff}
.help-form .location{padding:0 15px;margin-top:10px;background:#fff}
.help-form .location:before{margin-right:5px;}
.help-form > footer{position:absolute;margin:0;width:100%;-webkit-transform:translate3d(0,-100%,0);transform:translate3d(0,-100%,0);box-sizing:border-box}
